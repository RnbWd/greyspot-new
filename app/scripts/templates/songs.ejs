<div id="player" class="row alert alert-info">
    <div class="progress">
      <div class="progress-bar progress-bar-warning" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100">
        <span class="sr-only">60% Complete (warning)</span>
      </div>
    </div>
    <div class="col-md-5"><span id="song-title"></span></div>

    <div class="col-md-7">
   
        <button type="button" class="btn btn-default btn-sm" onclick="widget.prev()">
            <span class="glyphicon glyphicon-step-backward"></span>
        </button>

         <button type="button" class="btn btn-default btn-sm" onclick="widget.toggle()">
            <span class="glyphicon glyphicon-play player-play"></span>
        </button>

        <button type="button" class="btn btn-default btn-sm" onclick="widget.pause()">
            <span class="glyphicon glyphicon-stop"></span>
        </button>

        <button type="button" class="btn btn-default btn-sm" onclick="widget.next()">
            <span class="glyphicon glyphicon-step-forward"></span>
        </button>

   </div>
</div>

<section>

  <ul class="list-group">
      <!-- Where our To Do items will go -->
  </ul>

</section>